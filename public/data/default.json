{
  "datasource": {},
  "listeners": [],
  "fields": [
    {
      "component": "div",
      "children": [
        {
          "component": "el-form",
          "children": [
            {
              "component": "el-row",
              "children": [
                {
                  "component": "el-col",
                  "fieldOptions": {
                    "props": {
                      "span": "$:12"
                    }
                  },
                  "children": [
                    {
                      "component": "el-form-item",
                      "children": [
                        {
                          "component": "el-input",
                          "model": ["text1"]
                        },
                        {
                          "component": "span",
                          "fieldOptions": {
                            "domProps": {
                              "innerText": "$:model.text1"
                            }
                          }
                        }
                      ],
                      "fieldOptions": {
                        "props": {
                          "label": "text1"
                        }
                      }
                    }
                  ]
                },
                {
                  "component": "el-col",
                  "fieldOptions": {
                    "props": {
                      "span": "$:12"
                    }
                  },
                  "children": [
                    {
                      "component": "el-form-item",
                      "children": [
                        {
                          "component": "el-input",
                          "model": ["text2"]
                        },
                        {
                          "component": "span",
                          "fieldOptions": {
                            "domProps": {
                              "innerText": "$:(model.text2||'') + 'xxxxxxxxx'"
                            }
                          }
                        }
                      ],
                      "fieldOptions": {
                        "props": {
                          "label": "text2"
                        }
                      },
                      "_collapse": true
                    }
                  ]
                },
                {
                  "component": "el-col",
                  "fieldOptions": {
                    "props": {
                      "span": "$:16"
                    }
                  },
                  "children": [
                    {
                      "component": "el-form-item",
                      "children": [
                        {
                          "component": "el-input",
                          "model": ["text3"]
                        },
                        {
                          "component": "span",
                          "fieldOptions": {
                            "domProps": {
                              "innerText": "#:输入了 ${model.text3.length} 个字"
                            }
                          }
                        }
                      ],
                      "fieldOptions": {
                        "props": {
                          "label": "text3"
                        }
                      },
                      "_collapse": true
                    }
                  ]
                }
              ],
              "fieldOptions": {
                "props": {
                  "gutter": 20
                }
              }
            }
          ],
          "fieldOptions": {
            "props": {
              "labelWidth": "120px"
            }
          }
        },
        {
          "component": "button",
          "fieldOptions": {
            "domProps": {
              "innerText": "弹出"
            },
            "on": {
              "click": "@:alert('xxxxxx')"
            }
          }
        },
        {
          "component": "a",
          "fieldOptions": {
            "domProps": {
              "target": "_blank",
              "innerText": "打开链接"
            },
            "attrs": {
              "href": "http://www.baidu.com"
            }
          }
        },
        {
          "component": "hr"
        },
        {
          "component": "a",
          "fieldOptions": {
            "domProps": {
              "target": "_blank",
              "innerText": "打开text1输入的链接"
            },
            "attrs": {
              "href": "$:model.text1"
            }
          }
        }
      ]
    }
  ]
}
